<%= form_for [:admin, @category] do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <div class="fields">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  </div>
  <div class="fields">
    <%= f.submit t("button.submit"), class: "btn btn-primary" %>
  </div>
<% end %>
